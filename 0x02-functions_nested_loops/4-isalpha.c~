#include "holberton.h"

/**
 * _islower - checks for a lower-case character.
 * @c: ascii character to check for lower-case.
 * Return: 1, if lower-case.
 *         0, otherwise.
 */
int _islower(int c)
{

	if ((c >= 'A') && (c <= 'Z'))
	{
		return (1);
	}
	else if ((c >= 'a') && (c <= 'z'))
	{
		return (1);
	}
	else
		return (0);
}
